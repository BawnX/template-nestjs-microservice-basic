{
  "name": "template-nestjs-microservice",
  "title": "Template NestJS Microservices",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start:development": "npm run _deleteDist & cross-env NODE_ENV=development npm run _prismaGenerate & npm run _debug",
    "start:qa": "npm run _deleteDist & npm run _prismaGenerate & npm run _debug",
    "start:production": "npm run _deleteDist & cross-env NODE_ENV=production npm run _prismaGenerate & npm run _debug",
    "build:development": "cross-env NODE_ENV=development nest build",
    "build:qa": "cross-env NODE_ENV=qa nest build",
    "build:production": "cross-env NODE_ENV=production nest build",
    "migrate:dev": "npx prisma migrate dev",
    "migrate:dev:teo": "cross-env NODE_ENV=development npm run _prismaGenerateTEO & npx prisma migrate dev --name teo --schema prisma/schema_teo.prisma",
    "migrate:dev:create": "npx prisma migrate dev --create-only",
    "migrate:deploy": "npx prisma migrate deploy",
    "prisma:generate": "npx prisma generate --schema",
    "prisma:studio": "npx prisma studio",
    "prisma:seed": "npx prisma db seed --preview-feature",
    "_debug": "nest start --debug --watch",
    "_prismaSchemaTEO": "ts-node src/config/orm/teo.config.ts",
    "_prismaSchemaLegacy": "ts-node src/config/orm/legacy.config.ts",
    "_prismaGenerateTEO": "npm run _prismaSchemaTEO & npm run prisma:generate prisma/schema_teo.prisma",
    "_prismaGenerateLegacy": "npm run _prismaSchemaLegacy & npm run prisma:generate prisma/schema_legacy.prisma",
    "_prismaGenerate": "npm run _prismaGenerateTEO & npm run _prismaGenerateLegacy & npm run _copyPrisma",
    "_copyPrisma": "copyfiles prisma/**/* dist/",
    "_deleteDist": "rimraf ./dist"
  },
  "dependencies": {
    "@automapper/classes": "^6.2.1",
    "@automapper/core": "^6.2.1",
    "@automapper/nestjs": "^6.2.1",
    "@automapper/pojos": "^6.2.1",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/graphql": "^9.1.2",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/swagger": "^5.0.9",
    "@prisma/client": "^3.6.0",
    "apollo-server-express": "^3.5.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "graphql": "^15",
    "mssql": "^7.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.3",
    "@nestjs/testing": "^8.0.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.9.6",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "copyfiles": "^2.4.1",
    "env-cmd": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-decorator-position": "^2.2.29",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jest-formatting": "^3.0.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-radar": "^0.2.1",
    "jest": "27.2.1",
    "nock": "^13.1.1",
    "prisma": "^3.6.0",
    "prisma-nestjs-graphql": "^14.6.1",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
